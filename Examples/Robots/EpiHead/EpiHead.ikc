       class = "Compressor"
       name = "Compressor"
   />

   <module
     class = "BlobCreator"
     name = "BlobCreator"
   />

   <module
     class = "Constant"
     name = "Low"
     data = "0.3"
   />
   <module
     class = "Constant"
     name = "High"
     data = "0.9"
   />

   <module
     class = "Constant"
     name = "Angle"
     data = "151.0"
   />

   <module
     class = "TooClose"
     name = "TooClose"
   />

