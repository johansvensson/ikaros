<?xml version="1.0"?>

<group title="Epi Head">




    <description>
        WARNING. These modules is under development and can be change anytime!
	 </description>

   <module
     class = "BlobSearch"
     name = "BlobSearch"
   />

   <module
     class = "NerestPixel"
     name = "NerestPixel"
   />

